class Main {
	public static void main(String[] args) {
		Lenkehistorie frozen = new Lenkehistorie();
		frozen.nyttKapittel("Anna drar avgårde");
		frozen.nyttKapittel("Hesten rømmer");
		frozen.nyttKapittel("Kommer til hytta og møter Kristoffer");
		frozen.nyttKapittel("Ulver jager dem");
		frozen.nyttKapittel("Kommer til Elsa");
		frozen.nyttKapittel("Ismonster");
		frozen.nyttKapittel("Anna fryser nesten til is");
		frozen.nyttKapittel("Elsa redder Anna, Hans (slem) blir fengslet");
		
		frozen.skrivHistorie();
	}
}
